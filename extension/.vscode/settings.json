 {"svelte.plugin.typescript.enable": true,
  "svelte.plugin.typescript.diagnostics.enable": true,
  "svelte.plugin.css.diagnostics.enable": true,
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.diagnostics.enable": true,
  "editor.insertSpaces": false,
  "editor.tabSize": 2, 
  "[svelte]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2,
  },
  "[typescript]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2,
  },

"svelte.plugin.svelte.compilerWarnings": {
    "a11y-no-onchange": "ignore",
    "a11y-missing-attribute": "ignore",
    "a11y-missing-content": "ignore",
    "a11y-missing-heading": "ignore",
    "a11y-missing-interactive": "ignore",
    "a11y-missing-label": "ignore",
    "a11y-missing-redundant": "ignore",
    "a11y-missing-role": "ignore",
    "a11y-no-autofocus": "ignore",
    "a11y-no-distracting-elements": "ignore",
    "a11y-no-redundant-aria-roles": "ignore",
    "a11y-no-static-element-interactions": "ignore",
    "a11y-role-has-required-aria-props": "ignore",
    "a11y-tabindex-no-positive": "ignore"
},



}   